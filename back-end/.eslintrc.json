{
  "extends": "airbnb/base",
  "rules": {
    "no-console": "off",
    "no-alert": "off",
    "arrow-parens": [
      2,
      "always"
    ],
    "jsx-a11y/label-has-associated-control": 0,
    "quotes": [
      2,
      "single"
    ],
    "implicit-arrow-linebreak": "off",
    "consistent-return": "off",
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_",
        "ignoreRestSiblings": true
      }
    ],
    "object-curly-newline": "off"
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  }
}